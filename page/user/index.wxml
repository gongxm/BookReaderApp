
<view class="page">
  <view class="page-bd">
    <view class="weui-panel weui-panel-access">
      <view bindtap="navigate" class="weui-panel-bd" wx:if="{{thirdSession != ''}}">
        <view class="weui-media-box weui-media-box-appmsg" hover-class="weui-cell-active">
          <view class="weui-media-box-hd weui-media-box-hd-in-appmsg">
            <image class="weui-media-box-thumb user-cover" src="{{userInfo.avatarUrl}}" mode="aspectFill" />
          </view>

          <view class="weui-media-box-bd weui-media-box-bd-in-appmsg">
            <view class="weui-media-box-bd weui-media-box-bd-in-appmsg">
              <view class="weui-media-box-title">{{userInfo.nickName}}</view>
            </view>
          </view>
        </view>

        <view bindtap="navigateToTechnicianWallet" class="weui-cell weui-cell-access" hover-class="weui-cell-active">
          <view class="weui-cell-hd">
            <image src="{{icon}}" style="width: 28px;height: 28px;margin-right: 16px;vertical-align: middle;" />
          </view>
          <view class="weui-cell-bd weui-cell-primary">
            <view>城市:{{userInfo.city}}</view>
          </view>
          <view class="weui-cell-ft weui-cell-ft-in-access"></view>
        </view>


        <view bindtap="navigateToTechnicianWallet" class="weui-cell weui-cell-access" hover-class="weui-cell-active">
          <view class="weui-cell-hd">
            <image src="{{icon}}" style="width: 28px;height: 28px;margin-right: 16px;vertical-align: middle;" />
          </view>
          <view class="weui-cell-bd weui-cell-primary">
            <view>省份:{{userInfo.province}}</view>
          </view>
          <view class="weui-cell-ft weui-cell-ft-in-access"></view>
        </view>


        <view bindtap="navigateToTechnicianWallet" class="weui-cell weui-cell-access" hover-class="weui-cell-active">
          <view class="weui-cell-hd">
            <image src="{{icon}}" style="width: 28px;height: 28px;margin-right: 16px;vertical-align: middle;" />
          </view>
          <view class="weui-cell-bd weui-cell-primary">
            <view>国家:{{userInfo.country}}</view>
          </view>
          <view class="weui-cell-ft weui-cell-ft-in-access"></view>
        </view>


      </view>
      <view wx:else>
        <view bindtap="openUserInfoSetting" class="weui-media-box weui-media-box-appmsg" hover-class="weui-cell-active">
          <view class="weui-media-box-hd weui-media-box-hd-in-appmsg">
            <image class="weui-media-box-thumb" src="../../images/plus.png" mode="aspectFill" />
          </view>
          <view class="weui-media-box-bd weui-media-box-bd-in-appmsg">
            <view class="weui-media-box-title">授权登录</view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>