<view class="container">
  <view bindtap="navigate" class="weui-panel-bd" wx:if="{{thirdSession != ''}}">
    <view class="weui-grids">
      <view class="weui-grid" wx:for="{{categories}}" wx:key="name">
        <navigator url="{{item.url}}">
          <text class="weui-grid__label">{{item.name}}</text>
        </navigator>
      </view>
    </view>
  </view>

  <view wx:else>
    <view bindtap="openUserInfoSetting" class="weui-media-box weui-media-box-appmsg" hover-class="weui-cell-active">
      <view class="weui-media-box-hd weui-media-box-hd-in-appmsg">
        <image class="weui-media-box-thumb" src="../../images/plus.png" mode="aspectFill" />
      </view>
      <view class="weui-media-box-bd weui-media-box-bd-in-appmsg">
        <view class="weui-media-box-title">授权登录</view>
      </view>
    </view>
  </view>

</view>