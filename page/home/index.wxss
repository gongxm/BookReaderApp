.myipic {
  width: 100px;
  height: 120px;
  margin: 0 auto;
  margin-top: 5px;
}

.myipic image {
  display: block;
  width: 100%;
  height: 100%;
}

.title {
  display: block;
  text-align: center;
  font-weight: bold;
  color: #000;
  font-size: 14px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.mygrid {
  /* 上右下左 */
  margin: 10px 2px 0px 6px;
  border: 1px #ececec double;
  position: relative;
  float: left;
  width: 30%;
  text-align: center;
  padding: 0px;
  box-shadow: 5px 5px 7px rgba(22, 22, 22, 0.5);
}

.mygrid:active {
  background-color: #ececec;
}

.container {
  height: 15px;
  position: relative;
  float: left;
  width: 100%;
}

.plan {
  display: relative;
  margin: 10px;
}


.add_container {
  display: flex;
}

.input_text {
  border-radius: 5px;
  height: 30px;
  width: 100%;
  border: #888 1px double;
}

.plan_container {
  margin-top: 10px;
  padding: 10px;
  border: #000 1px double;
  border-radius: 5px;
}

.plan_item{
  margin: 10px;
}

.del_font{
 text-decoration: line-through;
}

.control{
  display: flex
}

.ctl_bt{
  margin-right: 30px;
  background: #eee;
  text-align: center;
  padding-top: 5px;
  border-radius: 5px;
  height: 30px;
  width:33%;
}
.ctl_bt:active {
  background: #888;
}